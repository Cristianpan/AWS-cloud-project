generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model Student {
  id            Int     @id @default(autoincrement())
  nombres       String
  apellidos     String
  matricula     String  
  promedio      Float   
  fotoPerfilUrl String?  @db.Text
  password      String
}

model Teacher {
  id             Int    @id @default(autoincrement())
  numeroEmpleado Int
  nombres        String
  apellidos      String
  horasClase     Int
}
